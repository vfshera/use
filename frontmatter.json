{
  "$schema": "https://beta.frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": "'components'",
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Slug",
          "name": "slug",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "publishDate",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Image",
          "name": "image",
          "type": "image"
        },
        {
          "title": "Framework",
          "name": "framework",
          "type": "string"
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Author",
          "name": "author",
          "default": "shera",
          "type": "string"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "astro",
  "frontMatter.content.publicFolder": "public",
  "frontMatter.preview.host": "http://localhost:3000",
  "frontMatter.content.pageFolders": [
    {
      "title": "docs",
      "path": "[[workspace]]/src/content/docs"
    }
  ],
  "frontMatter.custom.scripts": [
    {
      "id": "og-image",
      "title": "Generate OG Image",
      "script": "./scripts/og/og.cjs",
      "command": "~/.nvm/versions/node/v16.19.0/bin/node"
    }
  ]
}
